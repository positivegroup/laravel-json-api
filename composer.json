{
  "name": "cloudcreativity/laravel-json-api",
  "description": "JSON API (jsonapi.org) support for Laravel applications.",
  "keywords": [
    "jsonapi.org",
    "json-api",
    "jsonapi",
    "cloudcreativity",
    "json",
    "api",
    "laravel"
  ],
  "homepage": "https://github.com/cloudcreativity/laravel-json-api",
  "support": {
    "issues": "https://github.com/cloudcreativity/laravel-json-api/issues"
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Cloud Creativity Ltd",
      "email": "info@cloudcreativity.co.uk"
    },
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/positivegroup/utils-object"
    },
    {
      "type": "git",
      "url": "https://github.com/positivegroup/json-api-testing"
    },
    {
      "type": "git",
      "url": "https://github.com/positivegroup/json-api"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "cloudcreativity/utils-object": "dev-1.x-php81",
    "illuminate/console": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/pagination": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "laminas/laminas-diactoros": "^2.0|^3.0",
    "neomerx/json-api": "dev-v1.x-php81",
    "nyholm/psr7": "^1.5",
    "ramsey/uuid": "^3.0|^4.0",
    "symfony/psr-http-message-bridge": "^2.0|^7.0"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "cloudcreativity/json-api-testing": "dev-v2.1.0-php81",
    "composer/semver": "^1.5",
    "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
    "laravel/sanctum": "^3.2",
    "laravel/ui": "^4.2",
    "mockery/mockery": "~1.3.3|^1.4.2",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^7.5.15|^8.4|^9.3.3|^10.0|^11.0",
    "rector/rector": "^0.15.23"
  },
  "suggest": {
    "cloudcreativity/json-api-testing": "Required to use the test helpers."
  },
  "autoload": {
    "psr-4": {
      "CloudCreativity\\LaravelJsonApi\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CloudCreativity\\LaravelJsonApi\\Tests\\": "tests/lib",
      "Database\\Factories\\": "database/factories",
      "DummyApp\\": "tests/dummy/app",
      "DummyApp\\Factories\\": "tests/dummy/database/factories",
      "DummyApp\\Tests\\": "tests/dummy/tests",
      "DummyPackage\\": "tests/package/src",
      "DummyPackage\\Factories\\": "tests/package/database/factories"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-1.x-php80": "1.x-dev-php80"
    },
    "laravel": {
      "providers": [
        "CloudCreativity\\LaravelJsonApi\\ServiceProvider"
      ],
      "aliases": {
        "JsonApi": "CloudCreativity\\LaravelJsonApi\\Facades\\JsonApi"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
